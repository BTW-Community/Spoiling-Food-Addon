{
  "schemaVersion": 1,
  "id": "foodspoilmod",
  "version": "${version}",

  "name": "Food Spoil Addon",
  "description": "This mod does Food Spoiling plus a hundred other things",
  "authors": [
    "Arminias"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC-BY-4.0",
  "icon": "assets/foodspoilmod/icon.png",

  "environment": "*",
  "entrypoints": {
    "init": [
      "btw.community.arminias.foodspoil.FoodSpoilMod"
    ],
    "postInit": [
      "btw.community.arminias.foodspoil.FoodSpoilMod"
    ],
    "btw:addon": [
      "btw.community.arminias.foodspoil.FoodSpoilAddon"
    ]
  },
  "mixins": [
    "foodspoilmod.mixins.json"
  ],

  "accessWidener": "foodspoilmod.accesswidener",

  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "suggests": {
    "flamingo": "*"
  }
}
